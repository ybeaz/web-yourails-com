{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [""],
  "plugins": ["@typescript-eslint", "import"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "tsx": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parser": "@typescript-eslint/parser"
    }
  ],
  "rules": {
    "react/prop-types": 0,
    "import/prefer-default-export": "off"
  }
}
